programa {
  funcao inicio() {
    inteiro n1, n2, n3, media
    escreva("Digite o primeiro número \n")
    leia(n1)
    escreva("Digite o segundo número \n")
    leia(n2)
    escreva("Digite o terceiro número \n")
    leia(n3)

    media = (n1 + n2 + n3)/3

    escreva("A média dos três números é ",media)
  }
}
